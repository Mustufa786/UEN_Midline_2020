                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            style="@style/i_textview"
                            android:text="@string/f118" />

                        <RadioGroup
                            android:id="@+id/f118"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <RadioButton
                                android:id="@+id/f118a"
                                style="@style/radiobutton"
                                android:text="@string/f118a" />

                            <EditText
                                android:id="@+id/f118ax"
                                style="@style/radiobutton"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:hint="@string/f118a"
                                android:tag="f118a"
                                android:text='@{f118a.checked? f118ax.getText().toString() : ""}'
                                android:visibility='@{f118a.checked? View.VISIBLE : View.GONE}' />

                            <RadioButton
                                android:id="@+id/f118b"
                                style="@style/radiobutton"
                                android:text="@string/f118b" />

                            <EditText
                                android:id="@+id/f118bx"
                                style="@style/radiobutton"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:hint="@string/f118b"
                                android:tag="f118b"
                                android:text='@{f118b.checked? f118bx.getText().toString() : ""}'
                                android:visibility='@{f118b.checked? View.VISIBLE : View.GONE}' />
                        </RadioGroup>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <!-- f119  Radio   -->
                <androidx.cardview.widget.CardView
                    android:id="@+id/fldGrpCVf119"
                    style="@style/cardView">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            style="@style/i_textview"
                            android:text="@string/f119" />

                        <RadioGroup
                            android:id="@+id/f119"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <RadioButton
                                android:id="@+id/f119a"
                                style="@style/radiobutton"
                                android:text="@string/f119a" />

                            <RadioButton
                                android:id="@+id/f119b"
                                style="@style/radiobutton"
                                android:text="@string/f119b" />
                        </RadioGroup>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <!-- f120  Input-Numeric   -->
                <androidx.cardview.widget.CardView
                    android:id="@+id/fldGrpCVf120"
                    style="@style/cardView">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            style="@style/i_textview"
                            android:text="@string/f120" />

                        <com.edittextpicker.aliazaz.EditTextPicker
                            android:id="@+id/f120"
                            style="@style/EditTextAlphaNumeric"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="@string/f120"
