                            <RadioButton
                                android:id="@+id/f117c"
                                style="@style/radiobutton"
                                android:text="@string/f117c" />

                            <RadioButton
                                android:id="@+id/f117d"
                                style="@style/radiobutton"
                                android:text="@string/f117d" />

